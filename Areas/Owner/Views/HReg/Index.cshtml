@model RentQuest.Models.OwnerClass

@{
    ViewData["Title"] = "Index";
}

<head>
    <!--===============================================================================================-->
    <title>Register</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/images/icons/favicon.ico" />
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/bootstrap/css/bootstrap.min.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/animate/animate.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/css-hamburgers/hamburgers.min.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/animsition/css/animsition.min.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/select2/select2.min.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/vendor/daterangepicker/daterangepicker.css">
    <!--=========================================~/======================================================-->
    <link rel="stylesheet" type="text/css" href="~/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/css/main.css">
</head>
<body>
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        <div class="container">
            <a class="navbar-brand" href="index.html">P3S<span>Leasing</span></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item"><a class="nav-link" asp-area="Unregistered" asp-controller="Unreg" asp-action="Index">Home</a></li>
                    <li class="nav-item"><a class="nav-link" asp-area="Unregistered" asp-controller="Unreg" asp-action="Listing">Property</a></li>
                    <li class="nav-item cta"><a asp-area="Unregistered" asp-controller="Unreg" asp-action="Menu" class="nav-link ml-lg-2"><span class="icon-user"></span> Sign-In</a></li>
                    <li class="nav-item cta cta-colored"><a asp-area="Unregistered" asp-controller="Unreg" asp-action="Menu2" class="nav-link"><span class="icon-pencil"></span> Sign-Up</a></li>

                </ul>
            </div>
        </div>
    </nav>

    <div class="limiter">
        <div class="container-login100">

            <div class="wrap-login100">

                <form asp-action="Index" method="post" enctype="multipart/form-data">
                    <div class="login100-form">
                        <span class="login100-form-logo">
                            <i class="zmdi zmdi-home"></i>
                        </span>

                        <span class="login100-form-title p-b-34 p-t-27">
                            Sign Up
                        </span>

                        <div asp-validation-summary="ModelOnly" class="txt1 label-input100"></div>
                        <div class="form-group">
                            <label asp-for="HName" class="control-label zmdi zmdi-pin-account"></label>
                            <input asp-for="HName" class="form-control " placeholder="Name" />
                            <span asp-validation-for="HName" class="txt1 label-input100"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="HPwd" class="control-label zmdi zmdi-lock"></label>
                            <input asp-for="HPwd" class="form-control " placeholder="*************" />
                            <span asp-validation-for="HPwd" class="txt1"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="HRePwd" class="control-label zmdi zmdi-lock"></label>
                            <input asp-for="HRePwd" class="form-control " placeholder="*************" />
                            <span asp-validation-for="HRePwd" class="txt1"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="HEmail" class="control-label zmdi zmdi-email"></label>

                            <input asp-for="HEmail" class="form-control" placeholder="Email " />
                            <span asp-validation-for="HEmail" class="txt1"></span>
                            @*<p>@ViewBag.emailerror</p>*@
                        </div>

                        <div class="form-group">
                            <label asp-for="HPhone" class="control-label fa fa-phone"></label>
                            <input asp-for="HPhone" class="form-control " placeholder="Phone" />
                            <span asp-validation-for="HPhone" class="txt1"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="HGender" class="control-label fa fa-user"></label>
                            <br />

                            <label class="control-label" style="display: block;
    text-align: center; font-size: .85em;">Male</label>

                            <input asp-for="HGender" type="radio" class="form-control" value="M" />
                            <br />
                            <label class="control-label" style="display: block;
    text-align: center; font-size: .85em;">Female</label>

                            <input asp-for="HGender" type="radio" class="form-control" value="F" />
                            <span asp-validation-for="HGender" class="txt1"></span>
                        </div>

                        <div class="form-group">
                            <label asp-for="HDOB" class="control-label fa fa-calendar"></label>
                            <input asp-for="HDOB" type="date" class="form-control" />
                            <span asp-validation-for="HDOB" class="txt1"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="HImg" class="control-label zmdi zmdi-image-o"></label>
                            <input asp-for="HImg" type="file" name="file" class="form-control" />
                            <span asp-validation-for="HImg" class="txt1"></span>
                        </div>



                        <div class="form-group">
                            <div class="container-login100-form-btn">
                                <input type="submit" value="Sign Up" class="btn btn-primary login100-form-btn" />
                            </div>

                        </div>


                </form>
            </div>
            @Html.ViewData["HMessage"]
        </div>

    </div>
    </div>







    <script src="~/vendor/jquerylogin/jquery-3.2.1.min.js"></script>
    <!--=========~/======================================================================================-->
    <script src="~/vendor/animsition/js/animsition.min.js"></script>
    <!--=========~/======================================================================================-->
    <script src="~/vendor/bootstrap/js/popper.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--=========~/======================================================================================-->
    <script src="~/vendor/select2/select2.min.js"></script>
    <!--=========~/======================================================================================-->
    <script src="~/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/vendor/daterangepicker/daterangepicker.js"></script>
    <!--=========~/======================================================================================-->
    <script src="~/vendor/countdowntime/countdowntime.js"></script>
    <!--=========~/======================================================================================-->
    <script src="~/js/main.js"></script>

</body>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

