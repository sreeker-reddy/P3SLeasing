@model RentQuest.Models.CircTypes
@{
    ViewData["Title"] = "Dettails";
}
<br />

<h2 class="text-info">Circulation Type Details</h2>
<form asp-action="Dettails" method="post">
    <div class="p-4 rounded border">
        <div asp-validation-summary="ModelOnly" class="text-danger">

        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="CircType"></label>
            </div>
            <div class="col-5">
                <input asp-for="CircType" readonly="readonly" class="form-control" />
            </div>
            <span asp-validation-for="CircType" class="text-danger"></span>
        </div>
        <div class="form-group">

            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
            <a asp-action="Index" class="btn btn-success">Back To List</a>
            @*<input type="submit" class="btn btn-success" value="Back To List" />*@
        </div>
    </div>
</form>

@section Scripts
{
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial.cshtml");
    }
}
